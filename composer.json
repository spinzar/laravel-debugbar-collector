{
    "name": "spinzar/laravel-debugbar-collector",
  "description": "A laravel debugbar collector",
  "license": "MIT",
  "keywords": [
    "laravel",
    "debugbar",
    "collector"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Nasibullah Nassim",
            "email": "info@spinzar.co",
            "homepage": "https://spinzar.co",
            "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "barryvdh/laravel-debugbar": "^3.6",
    "illuminate/support": "^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Spinzar\\LaravelDebugbarCollector\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spinzar\\LaravelDebugbarCollector\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Spinzar\\LaravelDebugbarCollector\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
